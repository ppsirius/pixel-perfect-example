    "extract-text-webpack-plugin": "3.0.0",
    "node-sass": "4.5.3",
    "postcss-loader": "2.0.6",
    "rimraf": "2.6.1",
    "sass-loader": "6.0.6",
    "webpack": "3.5.5",
    "webpack-dev-server": "^3.0.0"
  },
  "browserslist": [
    "last 8 versions"
  ]
